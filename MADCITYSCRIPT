-- Script principal com autoexecução de dois scripts externos

print("Iniciando execução dos scripts externos...")

-- Função para tentar executar um script com segurança
local function executarScript(url)
    local sucesso, erro = pcall(function()
        loadstring(game:HttpGet(url, true))()
    end)

    if sucesso then
        print("Script carregado com sucesso:", url)
    else
        warn("Erro ao carregar script:", url, "\nErro:", erro)
    end
end

-- Script 1: Mad City GodMode
executarScript("https://raw.githubusercontent.com/Cesare0328/my-scripts/main/Mad%20City%20GodMode")

-- Você pode continuar seu código aqui
print("Todos os scripts foram executados.")
