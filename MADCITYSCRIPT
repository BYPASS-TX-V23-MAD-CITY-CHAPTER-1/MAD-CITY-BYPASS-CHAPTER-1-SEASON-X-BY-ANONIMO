-- FunÃ§Ã£o de OtimizaÃ§Ã£o de GrÃ¡ficos (Modo Minecraft)
local optimizeGraphics = Instance.new("TextButton", frame)
optimizeGraphics.Size = UDim2.new(0, 280, 0, 40)
optimizeGraphics.Position = UDim2.new(0, 10, 0, 200)
optimizeGraphics.Text = "Modo Potente (Minecraft)"
optimizeGraphics.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
optimizeGraphics.TextColor3 = Color3.fromRGB(255, 255, 255)

optimizeGraphics.MouseButton1Click:Connect(function()
    -- Remover texturas e sombras
    game.Lighting.GlobalShadows = false
    game.Lighting.FogEnd = 9e9
    game.Lighting.Ambient = Color3.fromRGB(255, 255, 255)
    game.Lighting.Brightness = 0
    game.Lighting.TimeOfDay = "14:00:00"
end)

-- Exibir FPS no topo da tela
local fpsDisplay = Instance.new("TextLabel", painel)
fpsDisplay.Size = UDim2.new(0, 100, 0, 30)
fpsDisplay.Position = UDim2.new(0, 10, 0, 10)
fpsDisplay.Text = "FPS: "
fpsDisplay.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
fpsDisplay.TextColor3 = Color3.fromRGB(255, 255, 255)

local function updateFPS()
    while true do
        fpsDisplay.Text = "FPS: " .. tostring(game:GetService("RunService").Heartbeat.FPS)
        wait(1)
    end
end

-- Iniciar contagem de FPS
updateFPS()

-- FunÃ§Ã£o de Reset a cada 3 minutos (loop)
local function autoReset()
    while true do
        wait(180) -- Espera 3 minutos
        game.Players.LocalPlayer.Character:BreakJoints()
    end
end

-- Iniciar reset automÃ¡tico
autoReset()

-- Minimizar painel
local minimizeButton = Instance.new("TextButton", frame)
minimizeButton.Size = UDim2.new(0, 40, 0, 40)
minimizeButton.Position = UDim2.new(0, 260, 0, 10)
minimizeButton.Text = "_"
minimizeButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
minimizeButton.TextColor3 = Color3.fromRGB(255, 255, 255)

minimizeButton.MouseButton1Click:Connect(function()
    painel.Visible = false
end)

-- Maximizar painel
local maximizeButton = Instance.new("TextButton", frame)
maximizeButton.Size = UDim2.new(0, 40, 0, 40)
maximizeButton.Position = UDim2.new(0, 260, 0, 50)
maximizeButton.Text = "+"
maximizeButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
maximizeButton.TextColor3 = Color3.fromRGB(255, 255, 255)

maximizeButton.MouseButton1Click:Connect(function()
    painel.Visible = true
end)

-- Exibir painel
painel.Enabled = true
