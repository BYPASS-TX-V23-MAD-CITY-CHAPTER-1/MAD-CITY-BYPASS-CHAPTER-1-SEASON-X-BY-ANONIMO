-- Seu script principal

print("Executando script principal...")

-- Autoexecução do script externo
local sucesso, erro = pcall(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/AdminEvilDRagon/Mad-Lads-V6-Revamp-GUI/refs/heads/main/Mad-Lads%20V6%20Revamp%20GUI", true))()
end)

if sucesso then
    print("Script externo carregado com sucesso!")
else
    warn("Erro ao carregar script externo:", erro)
end

-- Continuação do seu script
print("Tudo pronto!")
