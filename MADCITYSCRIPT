--[[ 
‚úÖ SCRIPT COMPLETO AN√îNIMO - TX
üîê Login por Key | ‚è∞ 120h | ‚öôÔ∏è Auto XP/Rop/Level | üßº Otimiza√ß√£o extrema | üîÑ Auto Reset
]]

-- CONFIGURA√á√ïES
local key_atual = "Duck"
local tempo_total_horas = 120
local keyFile = "anonimo_tx_keydata.txt"

if not writefile or not readfile then
    warn("Executor n√£o suporta writefile/readfile.")
    return
end

local function calcularTempoRestante(timestamp)
    local agora = os.time()
    local tempoUsado = agora - tonumber(timestamp)
    return math.max((tempo_total_horas * 3600) - tempoUsado, 0)
end

local autorizado, tempoRestante = false, tempo_total_horas * 3600
if isfile(keyFile) then
    local dados = readfile(keyFile)
    local partes = string.split(dados, "|")
    if partes[1] == key_atual then
        tempoRestante = calcularTempoRestante(partes[2])
        if tempoRestante > 0 then
            autorizado = true
        end
    end
end

-- UI de Login com Painel Integrado
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local function criarPainel()
    local ScreenGui = Instance.new("ScreenGui", game.CoreGui)
    ScreenGui.Name = "PainelAnonimoTX"

    local Main = Instance.new("Frame", ScreenGui)
    Main.Size = UDim2.new(0, 400, 0, 300)
    Main.Position = UDim2.new(0.5, -200, 0.5, -150)
    Main.BackgroundColor3 = Color3.fromRGB(25,25,25)
    Main.BorderSizePixel = 0

    local UICorner = Instance.new("UICorner", Main)
    UICorner.CornerRadius = UDim.new(0, 8)

    local UIList = Instance.new("UIListLayout", Main)
    UIList.Padding = UDim.new(0, 8)

    local titulo = Instance.new("TextLabel", Main)
    titulo.Size = UDim2.new(1, 0, 0, 40)
    titulo.Text = "üõ†Ô∏è PAINEL AN√îNIMO - TX"
    titulo.TextColor3 = Color3.new(1,1,1)
    titulo.BackgroundTransparency = 1
    titulo.Font = Enum.Font.GothamBold
    titulo.TextScaled = true

    local function criarBotao(txt, callback)
        local btn = Instance.new("TextButton", Main)
        btn.Size = UDim2.new(1, -20, 0, 40)
        btn.Position = UDim2.new(0, 10, 0, 0)
        btn.Text = txt
        btn.Font = Enum.Font.Gotham
        btn.TextScaled = true
        btn.BackgroundColor3 = Color3.fromRGB(0, 170, 127)
        btn.TextColor3 = Color3.new(1,1,1)
        btn.MouseButton1Click:Connect(callback)
        Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 6)
    end

    -- BOT√ïES DO PAINEL
    criarBotao("üéØ AUTO XP", function()
        game.StarterGui:SetCore("SendNotification", {
            Title = "AN√îNIMO - TX",
            Text = "ATEN√á√ÉO - AUTO BYPASS ATIVADO. USE A OP√á√ÉO AUTO XP!",
            Duration = 10
        })
        loadstring(game:HttpGet("https://raw.githubusercontent.com/AdminEvilDRagon/Mad-Lads-V6-Revamp-GUI/refs/heads/main/Mad-Lads%20V6%20Revamp%20GUI", true))()
        
        task.spawn(function() -- AUTO RESET
            while true do
                wait(180)
                LocalPlayer:LoadCharacter()
                print("Auto Reset Executado [3 min]")
            end
        end)
    end)

    criarBotao("üí∞ AUTO ROP", function()
        loadstring(game:HttpGet(('https://raw.githubusercontent.com/VanPotent/EpicHub/refs/heads/main/my/ar'),true))()
    end)

    criarBotao("‚ö° AUTO LEVEL 100", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Cesare0328/my-scripts/main/XP-FARM-1MIN.lua"))()
    end)

    criarBotao("üßº OTIMIZA√á√ÉO EXTREMA", function()
        for _, obj in ipairs(game:GetDescendants()) do
            if obj:IsA("Decal") or obj:IsA("Texture") or obj:IsA("ParticleEmitter") or obj:IsA("Trail") then
                obj:Destroy()
            elseif obj:IsA("BasePart") then
                obj.Material = Enum.Material.Plastic
                obj.Reflectance = 0
            end
        end
        settings().Rendering.QualityLevel = Enum.QualityLevel.Level01
        settings().Rendering.TextureQuality = Enum.TextureQuality.Low
        game.Lighting.GlobalShadows = false
        print("Modo Gr√°fico Feio ativado.")
    end)

    local nome = Instance.new("TextLabel", Main)
    nome.Size = UDim2.new(1, 0, 0, 30)
    nome.Text = "üë§ Jogador: " .. LocalPlayer.Name
    nome.TextColor3 = Color3.fromRGB(200, 200, 200)
    nome.BackgroundTransparency = 1
    nome.Font = Enum.Font.Gotham
    nome.TextScaled = true

    local tempo = Instance.new("TextLabel", Main)
    tempo.Size = UDim2.new(1, 0, 0, 30)
    tempo.TextColor3 = Color3.fromRGB(200, 200, 200)
    tempo.BackgroundTransparency = 1
    tempo.Font = Enum.Font.Gotham
    tempo.TextScaled = true

    task.spawn(function()
        while true do
            wait(1)
            local restante = math.floor(tempoRestante / 3600)
            tempo.Text = "‚è≥ Key v√°lida por " .. restante .. "h"
            tempoRestante -= 1
        end
    end)
end

if not autorizado then
    -- LOGIN
    local gui = Instance.new("ScreenGui", game.CoreGui)
    gui.Name = "LoginAnonimoTX"

    local frame = Instance.new("Frame", gui)
    frame.Size = UDim2.new(0, 250, 0, 120)
    frame.Position = UDim2.new(0.5, -125, 0.5, -60)
    frame.BackgroundColor3 = Color3.fromRGB(30,30,30)
    Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 8)

    local box = Instance.new("TextBox", frame)
    box.Size = UDim2.new(0.8, 0, 0, 35)
    box.Position = UDim2.new(0.1, 0, 0.2, 0)
    box.PlaceholderText = "Digite a Key"
    box.Text = ""
    box.TextScaled = true
    box.BackgroundColor3 = Color3.fromRGB(50,50,50)

    local btn = Instance.new("TextButton", frame)
    btn.Size = UDim2.new(0.6, 0, 0, 30)
    btn.Position = UDim2.new(0.2, 0, 0.65, 0)
    btn.Text = "Verificar"
    btn.BackgroundColor3 = Color3.fromRGB(0, 170, 127)
    btn.TextScaled = true

    btn.MouseButton1Click:Connect(function()
        if box.Text == key_atual then
            writefile(keyFile, key_atual.."|"..tostring(os.time()))
            gui:Destroy()
            criarPainel()
        else
            btn.Text = "Key Incorreta"
        end
    end)
else
    criarPainel()
end
