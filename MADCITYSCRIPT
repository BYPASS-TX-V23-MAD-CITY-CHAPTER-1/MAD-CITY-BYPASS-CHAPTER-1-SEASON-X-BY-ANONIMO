-- FunÃ§Ã£o de login com Key
local userKey = "anonimo-tx"
local sessionTime = 120 -- tempo restante em horas
local playerName = game.Players.LocalPlayer.Name
local keyVerified = false

-- FunÃ§Ã£o de verificaÃ§Ã£o de Key e exibiÃ§Ã£o do tempo restante
local function verifyKey()
    if userKey == "anonimo-tx" then
        keyVerified = true
        print("Key Verificada com Sucesso!")
        print("Tempo restante: " .. sessionTime .. " horas.")
    else
        keyVerified = false
        print("Chave invÃ¡lida.")
    end
end

verifyKey()

-- FunÃ§Ãµes do painel de controle
local painel = Instance.new("ScreenGui", game.Players.LocalPlayer.PlayerGui)
local frame = Instance.new("Frame", painel)
frame.Size = UDim2.new(0, 300, 0, 500)
frame.Position = UDim2.new(0, 10, 0, 10)
frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
frame.BorderSizePixel = 0

-- FunÃ§Ã£o de Auto XP
local autoXP = Instance.new("TextButton", frame)
autoXP.Size = UDim2.new(0, 280, 0, 40)
autoXP.Position = UDim2.new(0, 10, 0, 50)
autoXP.Text = "Ativar Auto XP"
autoXP.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
autoXP.TextColor3 = Color3.fromRGB(0, 0, 0)

autoXP.MouseButton1Click:Connect(function()
    print("ATENÃ‡ÃƒO - AUTO BYPASS ATIVADO USE A OPÃ‡ÃƒO AUTO XP QUE ESTÃ AUTORIZADA - ANÃ”NIMO TX")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/AdminEvilDRagon/Mad-Lads-V6-Revamp-GUI/refs/heads/main/Mad-Lads%20V6%20Revamp%20GUI", true))()
end)

-- FunÃ§Ã£o de Auto ROP
local autoROP = Instance.new("TextButton", frame)
autoROP.Size = UDim2.new(0, 280, 0, 40)
autoROP.Position = UDim2.new(0, 10, 0, 100)
