-- CONFIGURA√á√ÉO
local keyCorreta = "Duck"
local tempoLimite = 12 * 60 * 60 -- 12 horas em segundos
local HttpService = game:GetService("HttpService")
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer
local id = "TXDATA_" .. LocalPlayer.UserId
local data = {}

-- TEMPO
pcall(function()
    if isfile(id .. ".json") then
        local saved = readfile(id .. ".json")
        data = HttpService:JSONDecode(saved)
    end
end)

if not data.inicio then
    data.inicio = os.time()
    writefile(id .. ".json", HttpService:JSONEncode(data))
end

local tempoRestante = tempoLimite - (os.time() - data.inicio)
if tempoRestante <= 0 then
    game.StarterGui:SetCore("SendNotification", {
        Title = "TX Bypass",
        Text = "‚ùå Seu tempo acabou.",
        Duration = 6
    })
    return
end

-- GUI
local gui = Instance.new("ScreenGui", game.CoreGui)
gui.Name = "TX_Painel"

local frame = Instance.new("Frame", gui)
frame.Size = UDim2.new(0, 350, 0, 260)
frame.Position = UDim2.new(0.5, -175, 0.5, -130)
frame.BackgroundColor3 = Color3.fromRGB(20, 20, 35)
frame.BorderSizePixel = 0

local UICorner = Instance.new("UICorner", frame)
UICorner.CornerRadius = UDim.new(0, 12)

-- Gradiente
local gradient = Instance.new("UIGradient", frame)
gradient.Color = ColorSequence.new{
    ColorSequenceKeypoint.new(0, Color3.fromRGB(15, 25, 55)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(10, 80, 140))
}

-- T√≠tulo
local titulo = Instance.new("TextLabel", frame)
titulo.Size = UDim2.new(1, 0, 0, 50)
titulo.Text = "üîê TX ANT BYPASS 1.4"
titulo.Font = Enum.Font.GothamBold
titulo.TextSize = 20
titulo.TextColor3 = Color3.fromRGB(200, 255, 255)
titulo.BackgroundTransparency = 1

-- Campo de entrada da Key
local input = Instance.new("TextBox", frame)
input.PlaceholderText = "Digite sua Key..."
input.Size = UDim2.new(0.85, 0, 0, 30)
input.Position = UDim2.new(0.075, 0, 0, 120)
input.TextSize = 14
input.Font = Enum.Font.Gotham
input.Text = ""
input.ClearTextOnFocus = false
input.BackgroundColor3 = Color3.fromRGB(30, 30, 60)
input.TextColor3 = Color3.new(1,1,1)

local inputCorner = Instance.new("UICorner", input)
inputCorner.CornerRadius = UDim.new(0, 6)

-- Bot√£o de verifica√ß√£o da Key
local botao = Instance.new("TextButton", frame)
botao.Size = UDim2.new(0.85, 0, 0, 35)
botao.Position = UDim2.new(0.075, 0, 0, 165)
botao.Text = "‚úÖ Verificar Key"
botao.TextColor3 = Color3.new(1,1,1)
botao.Font = Enum.Font.GothamBold
botao.TextSize = 14
botao.BackgroundColor3 = Color3.fromRGB(0, 200, 80)

local botaoCorner = Instance.new("UICorner", botao)
botaoCorner.CornerRadius = UDim.new(0, 6)

-- Status
local status = Instance.new("TextLabel", frame)
status.Size = UDim2.new(1, 0, 0, 20)
status.Position = UDim2.new(0, 0, 1, -25)
status.Text = "üîê Aguardando Key..."
status.Font = Enum.Font.Gotham
status.TextColor3 = Color3.fromRGB(255, 255, 100)
status.BackgroundTransparency = 1
status.TextSize = 13

-- Fun√ß√£o de clique
botao.MouseButton1Click:Connect(function()
    if input.Text == keyCorreta then
        status.Text = "‚úÖ Key aceita. Exibindo painel de manuten√ß√£o..."
        wait(1)
        frame:Destroy()

        -- Painel de manuten√ß√£o
        local maintenanceFrame = Instance.new("Frame", gui)
        maintenanceFrame.Size = UDim2.new(0, 400, 0, 300)
        maintenanceFrame.Position = UDim2.new(0.5, -200, 0.5, -150)
        maintenanceFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 40)
        maintenanceFrame.BorderSizePixel = 0

        local maintenanceCorner = Instance.new("UICorner", maintenanceFrame)
        maintenanceCorner.CornerRadius = UDim.new(0, 12)

        -- Avatar do jogador
        local avatar = Instance.new("ImageLabel", maintenanceFrame)
        avatar.Size = UDim2.new(0, 100, 0, 100)
        avatar.Position = UDim2.new(0.5, -50, 0, 20)
        avatar.BackgroundTransparency = 1
        avatar.Image = Players:GetUserThumbnailAsync(LocalPlayer.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size100x100)

        -- Nome do jogador
        local nome = Instance.new("TextLabel", maintenanceFrame)
        nome.Size = UDim2.new(1, 0, 0, 30)
        nome.Position = UDim2.new(0, 0, 0, 130)
        nome.Text = "Usu√°rio: " .. LocalPlayer.Name
        nome.Font = Enum.Font.GothamBold
        nome.TextSize = 18
        nome.TextColor3 = Color3.fromRGB(200, 255, 255)
        nome.BackgroundTransparency = 1

        -- Tempo restante
        local horas = math.floor(tempoRestante / 3600)
        local minutos = math.floor((tempoRestante % 3600) / 60)
        local tempoLabel = Instance.new("TextLabel", maintenanceFrame)
        tempoLabel.Size = UDim2.new(1, 0, 0, 20)
        tempoLabel.Position = UDim2.new(0, 0, 0, 170)
        tempoLabel.Text = "Tempo restante: " .. horas .. "h " .. minutos .. "m"
        tempoLabel.Font = Enum.Font.Gotham
        tempoLabel.TextSize = 14
        tempoLabel.TextColor3 = Color3.fromRGB(170, 255, 170)
        tempoLabel.BackgroundTransparency = 1

        -- FPS
        local fpsLabel = Instance.new("TextLabel", maintenanceFrame)
        fpsLabel.Size = UDim2.new(1, 0, 0, 20)
        fpsLabel.Position = UDim2.new(0, 0, 0, 200)
        fpsLabel.Text = "FPS: Calculando..."
        fpsLabel.Font = Enum.Font.Gotham
        fpsLabel.TextSize = 14
        fpsLabel.TextColor3 = Color3.fromRGB(255, 255, 100)
        fpsLabel.BackgroundTransparency = 1

        -- Mensagem de manuten√ß√£o
        local mensagem = Instance.new("TextLabel", maintenanceFrame)
        mensagem.Size = UDim2.new(1, -20, 0, 60)
        mensagem.Position = UDim2.new(0, 10, 0, 230)
        mensagem.Text = "üõ†Ô∏è Estamos em manuten√ß√£o\nMotivo: Corrigindo bugs e atualizando o bypass"
        mensagem.Font = Enum.Font.GothamBold
        mensagem.TextSize = 16
        mensagem.TextColor3 = Color3.fromRGB(255, 100, 100)
        mensagem.BackgroundTransparency = 1
        mensagem.TextWrapped = true

        -- Atualiza√ß√£o do FPS
        local lastTime = tick()
        local frames = 0
        RunService.RenderStepped:Connect(function()
            frames = frames + 1
            local currentTime = tick()
            if currentTime - lastTime >= 1 then
                fpsLabel.Text = "FPS: " .. frames
                frames = 0
                lastTime = currentTime
            end
        end)

    else
        status.Text = "‚ùå Key inv√°lida"
        status.TextColor3 = Color3.fromRGB(255, 80, 80)
    end
end)
